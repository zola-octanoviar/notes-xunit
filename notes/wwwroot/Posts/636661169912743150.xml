&lt;li class="" style="box-sizing: border-box; border-top: none; border-right: none; border-bottom: none; border-left: 3px solid #6ce26c; border-image: initial; list-style-type: decimal-leading-zero; list-style-image: initial; background-color: #f8f8f8; line-height: 14px; margin: 0px !important; padding: 0px 3px 0px 10px !important; list-style-position: outside !important;"&gt;&lt;span style="box-sizing: border-box; margin: 0px; padding: 0px; border: none; color: black; background-color: inherit;"&gt;&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/li&gt;
&lt;li class="alt" style="box-sizing: border-box; border-top: none; border-right: none; border-bottom: none; border-left: 3px solid #6ce26c; border-image: initial; list-style-type: decimal-leading-zero; list-style-image: initial; color: inherit; line-height: 14px; margin: 0px !important; padding: 0px 3px 0px 10px !important; list-style-position: outside !important;"&gt;&lt;span style="box-sizing: border-box; margin: 0px; padding: 0px; border: none; color: black; background-color: inherit;"&gt;&lt;span class="keyword" style="box-sizing: border-box; margin: 0px; padding: 0px; border: none; color: #006699; background-color: inherit; font-weight: bold;"&gt;namespace&lt;/span&gt;&lt;span style="box-sizing: border-box; margin: 0px; padding: 0px; border: none; background-color: inherit;"&gt;&amp;nbsp;SampleMVCApps.Controllers&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
